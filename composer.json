{
    "name": "zenstruck/controller-util",
    "type": "library",
    "description": "Utilities for creating Symfony2 responses without dependencies.",
    "keywords": ["symfony", "silex", "symfony2", "controller", "response", "rest", "serializer"],
    "homepage": "http://zenstruck.com/projects/ControllerUtil",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/http-kernel": "~2.3|~3.0"
    },
    "require-dev": {
        "symfony/routing": "~2.3|3.0",
        "symfony/templating": "~2.3|3.0",
        "jms/serializer": "~0.16|~1.0"
    },
    "suggest": {
        "symfony/templating": "For using the TemplatingViewListener",
        "symfony/routing": "For using the RedirectListener",
        "twig/twig": "For using the TwigViewListener",
        "jms/serializer": "For using the SerializerViewListener"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\ControllerUtil\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\ControllerUtil\\Tests\\": "tests/" }
    }
}
